# YOLO Model Configuration
# Options: yolov8n.pt (nano), yolov8s.pt (small), yolov8m.pt (medium), yolov8l.pt (large), yolov8x.pt (xlarge)
MODEL_NAME=yolov8m.pt

# Video Source Configuration
# Examples:
# Local file: sample.mp4 or videos/traffic.mp4
# RTSP stream: rtsp://username:password@192.168.1.100:554/stream1
# HTTP stream: http://example.com/live/stream.m3u8
# Webcam: 0 (default camera), 1, 2, etc.
VIDEO_SOURCE=sample.mp4

# Frame Processing
# FRAME_SKIP: Process every Nth frame (1=all frames, 2=every 2nd frame, 3=every 3rd, etc.)
# Higher values = faster processing but may miss objects
FRAME_SKIP=3

# Detection Settings
# CONFIDENCE_THRESHOLD: Minimum confidence score (0.0 to 1.0)
CONFIDENCE_THRESHOLD=0.5

# Target Classes (comma-separated, leave empty for all classes)
# Common classes: person,car,truck,bus,motorcycle,bicycle,traffic light,stop sign
TARGET_CLASSES=car

# Display Settings
# DISPLAY_VIDEO: Show video window with detections (true/false)
DISPLAY_VIDEO=false

# Output Settings
SAVE_OUTPUT=false
OUTPUT_PATH=output/

# MongoDB Configuration
ENABLE_MONGODB=true
MONGO_URI=mongodb://localhost:27017/
MONGO_DB_NAME=yolo_detections
MONGO_COLLECTION_NAME=detections